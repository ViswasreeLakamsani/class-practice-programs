<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="emp1" class="com.kodnest.beans.Employee">

<property name="technologies">
<list>
<value>Java</value>
<value>Python</value>
<value>Sql</value>
</list>
</property>
<property name="project">
<set>
<value>project-01</value>
<value>project-02</value>
<value>project-03</value>
</set>
</property>
<property name="organization">
<map>
<entry key="org-01" value="Tcs"/>
<entry key="org-02" value="infosys"/>
<entry key="org-03" value="wipro"/>
<entry key="org-04" value="Hcl"/>
</map>
</property>
</bean>
</beans>