<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

 <bean id="l1" class=" com.kodnest.bean.Laptop  ">
  <property name="id" value="101" />
  <property name="model" value="Dell"/>
  <property name="cost" value="67000" />
 </bean>

 <bean id="l2" class=" com.kodnest.bean.Laptop  ">
  <constructor-arg name="id"  value="" /> 
  <constructor-arg name="model" value="Hp"/>
  <constructor-arg name="cost" value="69000" />
 </bean>
 
</beans>